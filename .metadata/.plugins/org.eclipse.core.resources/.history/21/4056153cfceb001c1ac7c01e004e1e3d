import java.util.Locale;
import java.util.Scanner;

public class entrada {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int A ,B ,Result;
		A = sc.nextInt();
		B = sc.nextInt();
		
		Result = A + B;
		
		System.out.print(Result);
		
	
		sc.close();
	}

}
