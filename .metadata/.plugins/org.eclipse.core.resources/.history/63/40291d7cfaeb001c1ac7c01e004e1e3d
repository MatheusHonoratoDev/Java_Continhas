import java.util.Locale;
import java.util.Scanner;

public class entrada {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int a, b, soma; 
		a = 10;
		b = 30;
		soma = a + b;
		
		System.out.println(soma);
		
		int a, b, soma;
		a = -30;
		b = 10;
		soma = a + b;
		
		System.out.println(soma);
		
		


	}

}
